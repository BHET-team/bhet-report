@article{quinn2018,
  title = {An Analysis of Efforts to Scale up Clean Household Energy for Cooking around the World},
  author = {Quinn, Ashlinn K. and Bruce, Nigel and Puzzolo, Elisa and Dickinson, Katherine and Sturke, Rachel and Jack, Darby W. and Mehta, Sumi and Shankar, Anita and Sherr, Kenneth and Rosenthal, Joshua P.},
  year = {2018},
