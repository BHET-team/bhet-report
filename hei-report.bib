@incollection{callaway2020,
  title = {Difference-in-{{Differences}} for {{Policy Evaluation}}},
  booktitle = {Handbook of {{Labor}}, {{Human Resources}} and {{Population Economics}}},
  author = {Callaway, Brantly},
