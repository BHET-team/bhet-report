@article{meng2019,
  title = {Energy and Air Pollution Benefits of Household Fuel Policies in Northern {{China}}},
  author = {Meng, Wenjun and Zhong, Qirui and Chen, Yilin and Shen, Huizhong and Yun, Xiao and Smith, Kirk R. and Li, Bengang and Liu, Junfeng and Wang, Xilong and Ma, Jianmin and Cheng, Hefa and Zeng, Eddy Y. and Guan, Dabo and Russell, Armistead G. and Tao, Shu},
