@article{li2022,
  title = {Socioeconomic and {{Demographic Associations}} with {{Wintertime Air Pollution Exposures}} at {{Household}}, {{Community}}, and {{District Scales}} in {{Rural Beijing}}, {{China}}},
  author = {Li, Xiaoying and Baumgartner, Jill and {Barrington-Leigh}, Christopher and Harper, Sam and Robinson, Brian and Shen, Guofeng and Sternbach, Talia and Tao, Shu and Zhang, Xiang and Zhang, Yuanxun and Carter, Ellison},
  year = {2022},
